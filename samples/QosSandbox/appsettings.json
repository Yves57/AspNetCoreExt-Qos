{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },

  "Vip": {
    "IpAddresses": [
      "11.22.33.44",
      "22.33.44.55"
    ]
  },

  "Concurrency": {
    "Policies": {
      "C1": {
        "UrlTemplates": [ "*" ],
        "Key": "@(\"Global\")",
        "MaxCount": 2
      }
    }
  },

  "RateLimit": {
    "Policies": {
      "R1": {
        "UrlTemplates": [ "/api/ratelimit/{id}" ],
        "Key": "@(context.Request.IpAddress)",
        "MaxCount": 2,
        "Period":  "0:0:10"
      },
      "R_MVC": {
        "Key": "@(context.Request.IpAddress)",
        "MaxCount": 2,
        "Period": "0:0:10"
      }
    }
  },

  "Quota": {
    "Policies": {
      "Q1": {
        "UrlTemplates": [ "/api/quota1", "/api/quota2" ],
        "Key": "@(context.Request.Url)",
        "MaxCount": 30000,
        "Period": "0:0:30"
      }
    }
  }
}
