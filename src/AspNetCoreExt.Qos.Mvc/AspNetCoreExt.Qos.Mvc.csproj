<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>AspNetCoreExt.Qos.Mvc</PackageId>
    <Version>1.0.0</Version>
    <Authors>Yves Marx</Authors>
    <Company>-</Company>
    <Description>ASP.NET Core QoS is a set of high-performance and highly customizable middlewares that allow to set different limits on requests (quotas, rates, concurrency,...).</Description>
    <Copyright>Copyright Â© Yves Marx 2018</Copyright>
    <PackageLicenseUrl>https://raw.githubusercontent.com/Yves57/AspNetCoreExt-Qos/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Yves57/AspNetCoreExt-Qos</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Yves57/AspNetCoreExt-Qos</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>aspnetcore;concurrency;rate limit;quota;qos</PackageTags>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <InformationalVersion>1.0.0</InformationalVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.ApiExplorer" Version="2.1.1" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.1.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\AspNetCoreExt.Qos.Abstractions\AspNetCoreExt.Qos.Abstractions.csproj" />
  </ItemGroup>

</Project>
